#!/bin/bash

cd $IOTKITDEPEND

LOGSTART1 "Informix Product"
##################################
# Install Informix Product
##################################

cd $IOTKITDEPEND
sudo wget http://nosqldemo.com/gateway-kit/quark/informix.tar.gz
sudo mkdir /opt/IBM
sudo cp $IOTKITDEPEND/informix.tar.gz /opt/IBM/
cd /opt/IBM
sudo tar xzvf /opt/IBM/informix.tar.gz

cd $IOTKITDIR

LOGSTOP1
