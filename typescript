]0;pi@raspberrypi: ~/iot-gateway-kit[01;32mpi@raspberrypi[00m [01;34m~/iot-gateway-kit $[00m ./inst[K[K[K[K[K[Kmore inst
while true
do
SZ1=`ls -l ../iot-gateway-kit-depend/*.tar|awk '{print $5}'`
sleep 5
SZ2=`ls -l ../iot-gateway-kit-depend/*.tar|awk '{print $5}'`
echo "$SZ1 $SZ2"

if [ $SZ1 != $SZ2 ]
then

echo "not same "
else
echo "same "
break
fi
done

./iot_install
